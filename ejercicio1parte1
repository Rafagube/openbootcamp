package Ejercicio1;

public class practica {
	
	//Damos los valores a la funcion y comprobamos que esta correcto
	
	public static void main(String[] args) {
		
		int total = 0;
		
		total= sum(6, 3, 9);
		
		System.out.println(total);
		
	}
	
	//Creamos la funcion sum que har√° la suma 
	
	public static int sum(int a, int b, int c) {
		
		return (a + b + c );
	}
	
}
